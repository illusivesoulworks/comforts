{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "issues": "${issue_tracker}"
  },
  "license": "${license}",
  "icon": "${mod_id}_icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.illusivesoulworks.comforts.ComfortsFabricMod"
    ],
    "client": [
      "com.illusivesoulworks.comforts.ComfortsFabricClientMod"
    ],
    "spectrelib": [
      "com.illusivesoulworks.comforts.ComfortsFabricConfig"
    ],
    "cardinal-components-entity": [
      "com.illusivesoulworks.comforts.common.ComfortsComponents"
    ]
  },
  "custom": {
    "cardinal-components": [
      "comforts:sleep_tracker"
    ]
  },
  "mixins": [
    "comforts.mixins.json",
    "comforts.fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14",
    "fabric": "*",
    "minecraft": "${fabric_mc_version_range}",
    "java": ">=17"
  }
}
  